{
  "name": "feathers-starter-react-redux-login",
  "version": "0.0.3",
  "description": "React+Redux+Feathersjs boilerplate with a functional local login supporting email verification and password reset.",
  "scripts": {
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint client/. common/. config/. server/. test/. --ext .js,.jsx .",
    "prebuild": "rm -rf ./public/dist",
    "build": "webpack --progress --colors",
    "buildxxx": "NODE_ENV=production webpack --history-api-fallback --progress --colors | grep -v  \"\\[built\\]\"",
    "prebuild:dev": "rm -rf ./server/public/dist",
    "build:dev": "webpack --history-api-fallback --progress --colors | grep -v  \"\\[built\\]\"",
    "build:devserver": "NODE_ENV=dev_server webpack-dev-server --content-base server/public/dist/ | grep -v  \"\\[built\\]\"",
    "start": "node server/",
    "mocha": "mocha test/ --recursive",
    "npm:patch": "npm version patch && npm publish",
    "npm:minor": "npm version minor && npm publish",
    "npm:major": "npm version major && npm publish",
    "npm:updates": "npm-check-updates"
  },
  "scriptsxxx": {
    "copy": "rm -rf lib/ && cp -r src/ lib/",
    "babel": "babel src/ --out-dir lib --presets babel-preset-es2015,babel-preset-stage-0",
    "babel:watch": "babel src/ --watch --out-dir lib --presets babel-preset-es2015,babel-preset-stage-0",
    "mocha": "mocha test/  --reporter spec --compilers js:babel-core/register",
    "mocha:es6": "mocha test/ --reporter spec",
    "eslint": "eslint src/. test/. --ext .js,.jsx .",
    "build": "npm run copy && npm run babel",
    "test": "npm run build && npm run eslint && npm run mocha",
    "test:es6": "npm run eslint && npm run mocha:es6",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --reporter spec test/*",
    "watch": "npm run copy && npm run babel:watch",
    "git:push": "npm run build && git push origin && git push origin --tags",
    "npm:patch": "npm version patch && npm publish",
    "npm:minor": "npm version minor && npm publish",
    "npm:major": "npm version major && npm publish",
    "npm:updates": "npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddyystop/feathers-starter-react-redux-login.git"
  },
  "keywords": [
    "feathers",
    "feathersjs",
    "hook",
    "hooks",
    "react",
    "redux",
    "starter",
    "boilerplate",
    "login"
  ],
  "author": "John Szwaronek <johnsz9999@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eddyystop/feathers-starter-react-redux-login/issues"
  },
  "homepage": "https://github.com/eddyystop/feathers-starter-react-redux-login#readme",
